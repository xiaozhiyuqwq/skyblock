#单方块刷新点保护
execute @e[type=leash_knot,tag=ob] ~ ~ ~ scoreboard players set @a[r=35] actionbar 35
execute @a[scores={actionbar=35},tag=!onsetwarn] ~ ~ ~ tellraw @s[scores={language=0}] {"rawtext":[{"text":"§l§e空岛生存§f>>§r§7您已经接近刷新点。请不要采用特殊方案破坏刷新点，否则将会产生不可预知的错误以及导致刷新失败。"}]}
execute @a[scores={actionbar=35},tag=!onsetwarn] ~ ~ ~ tellraw @s[scores={language=1}] {"rawtext":[{"text":"§l§eSkyBlock§f>>§r§7You are close to the refresh point. Please do not use special schemes to destroy the refresh point, otherwise unpredictable errors will occur and refresh will fail."}]}
execute @a[scores={actionbar=35},tag=!onsetwarn] ~ ~ ~ tellraw @s[scores={language=2}] {"rawtext":[{"text":"§l§e空島生存§f>>§r§7您已經接近重繪點。請不要採用特殊方案破壞重繪點，否則將會產生不可預知的錯誤以及導致重繪失敗。"}]}
execute @a[scores={actionbar=35},tag=!onsetwarn] ~ ~ ~ tellraw @s[scores={language=3}] {"rawtext":[{"text":"§l§eSkyBlock§f>>§r§7リフレッシュ ポイントに近づいています。 特別なシナリオでリフレッシュ ポイントを破壊しないでください。"}]}
execute @a[scores={actionbar=35}] ~ ~ ~ tag @s add onsetwarn
execute @a[scores={actionbar=!35}] ~ ~ ~ tag @s remove onsetwarn
execute @a[scores={actionbar=35}] ~ ~ ~ titleraw @s[scores={language=0}] actionbar {"rawtext":[{"text":"§l§e请不要破坏刷新点 否则将会产生§c不可预知的错误§e！"}]}
execute @a[scores={actionbar=35}] ~ ~ ~ titleraw @s[scores={language=1}] actionbar {"rawtext":[{"text":"§ePlease do not destroy the refresh point, otherwise unpredictable errors will occur!"}]}
execute @a[scores={actionbar=35}] ~ ~ ~ titleraw @s[scores={language=2}] actionbar {"rawtext":[{"text":"§l§e請不要破壞重繪點 否則將會產生§c不可預知的錯誤§e！"}]}
execute @a[scores={actionbar=35}] ~ ~ ~ titleraw @s[scores={language=3}] actionbar {"rawtext":[{"text":"§l§eリフレッシュポイントを破壊しないと予知できないエラーが発生します。"}]}
